{
  "name": "Boxscore-API",
  "description": "API to consume boxscores",
  "dependencies": {
    "mongodb": "^3.1.10",
    "mongoose": "^5.3.13",
    "node-fetch": "^2.3.0"
  },
  "devDependencies": {
